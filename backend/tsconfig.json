{
  "compilerOptions": {
      "module": "commonjs",
      "target": "es2016",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "skipLibCheck": true,
      "paths": {
          "*": [
              "./node_modules/*",
          ]
      }
  },
  "include": [
      "./src/**/*",
      "./src/*"
  ]
}